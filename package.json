{
	"name": "nito",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack -p 3210",
		"build": "next build",
		"start": "next start -p 3210",
		"lint": "next lint",
		"format": "biome format --write",
		"check": "biome check --write",
		"commitlint": "commitlint --edit $1",
		"test": "vitest --run",
		"test:watch": "vitest watch",
		"test:ui": "vitest --ui",
		"test:update": "vitest --run --update",
		"test:update:watch": "vitest --watch --update",
		"test:coverage": "vitest run --coverage",
		"test:coverage:open": "vitest run --coverage && open coverage/index.html",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui",
		"e2e:watch": "PWTEST_WATCH=1 playwright test",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"imports": {
		"#entities/*": {
			"storybook": "./src/entities/*.mock.ts",
			"default": "./src/entities/*.ts"
		},
		"#features/*": {
			"storybook": "./src/features/*.mock.ts",
			"default": "./src/features/*.ts"
		},
		"#shared/*": {
			"storybook": "./src/shared/*.mock.ts",
			"default": "./src/shared/*.ts"
		}
	},
